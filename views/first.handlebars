{{#each blogData as |blog|}}
<div class="card">
    <h5 class="card-header">{{blog.title}}</h5>
    <div class="card-body">
      <h5 class="card-title">{{blog.user_name}}</h5>
      <p class="card-text">{{blog.contents}}</p>
      <a href="#" class="btn btn-primary">Go somewhere</a>
    </div>
  </div>
{{/each}}